{
	"name": "bayder-school-blocks",
	"version": "1.0.0",
	"private": false,
	"description": "custom wordpress gutengerg blocks for bayder-school theme",
	"repository": "git+https://github.com/seryozhko/bayder-school-blocks.git",
	"author": "seryozhko",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"WordPress",
		"editor",
		"Gutenberg"
	],
	"scripts": {
		"lint-js": "eslint .",
		"lint-js:fix": "eslint . --fix",
		"build": "npm run build:block-bio && npm run build:block-carousel && npm run build:block-map && npm run build:block-schedule",
		"build:block-bio": "cd block-bio && npm run build",
		"build:block-carousel": "cd block-carousel && npm run build",
